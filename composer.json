{
    "name": "tomatophp/filament-plugins",
    "type": "library",
    "description": "Manage your modules as a plugin system with plugin generator",
    "keywords": [
        "php",
        "laravel",
        "template",
        "modules",
        "HMVC",
        "filament",
        "plugins"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\FilamentPlugins\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\FilamentPlugins\\FilamentPluginsServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "tomatophp/console-helpers": "^1.0"
    }
}
